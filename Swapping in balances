#include <stdio.h>
int main() {
    void swap(float *x, float *y) {
    float temp = *x;
    *x = *y;
    *y = temp;
}
    float a, b;
    printf("Enter two balances: ");
    scanf("%f %f", &a, &b);
    printf("Before Swap: a = %.2f, b = %.2f\n", a, b);
    swap(&a, &b);
    printf("After Swap:  a = %.2f, b = %.2f\n", a, b);
    return 0;
}
